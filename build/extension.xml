<extension xmlns="http://ns.adobe.com/air/extension/3.5">
	<id>com.davikingcode.nativeExtensions.GooglePlus</id>
	<versionNumber>0.1</versionNumber>
	<platforms>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libGooglePlusIosExtension.a</nativeLibrary>
				<initializer>GooglePlusExtensionInitializer</initializer>
				<finalizer>GooglePlusExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>GooglePlusAndroidExtension.jar</nativeLibrary>
				<initializer>com.davikingcode.nativeExtensions.googlePlus.GooglePlusExtension</initializer>
				<finalizer>com.davikingcode.nativeExtensions.googlePlus.GooglePlusExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>GooglePlusAndroidExtension.jar</nativeLibrary>
				<initializer>com.davikingcode.nativeExtensions.googlePlus.GooglePlusExtension</initializer>
				<finalizer>com.davikingcode.nativeExtensions.googlePlus.GooglePlusExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default">
			<applicationDeployment/>
		</platform>
	</platforms>
</extension>
